{
    "name": "magento/project-community-edition",
    "description": "eCommerce Platform for Growth (Community Edition)",
    "type": "project",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "amasty/geoipredirect": "^1.3",
        "bezigeboefjes/magento2-extension-change-attribute-set": "^1.0",
        "boldcommerce/magento2-ordercomments": "^1.6",
        "code4business/freeproduct2": "^1.2",
        "copex/vatfix": "^1.0",
        "e-conomix/module-dbtranslations": "^1.0",
        "espl/onepagecheckout": "^2.0",
        "firegento/magesetup2": "^0.1.2",
        "ingenico/ingenico_epayments_ogn2": "^3.5",
        "limesharp/stockists": "^2.0",
        "lucasart/stock-status-label": "^1.2",
        "magefan/module-translation": "^2.0",
        "magekey/module-product-slider-widget": "^2.0",
        "magento/data-migration-tool": "2.3.2",
        "magento/product-community-edition": "2.3.2",
        "mageplaza/magento-2-product-slider": "^2.0",
        "mageplaza/module-banner-slider": "^2.0",
        "mageplaza/module-currency-formatter": "^1.0",
        "mageplaza/module-instagram-feed": "^1.0",
        "mageplaza/module-sitemap": "^1.0",
        "mageworx/module-searchsuiteautocomplete": "^2.1",
        "patrickfuchshofer/giftvoucher": "^1.0",
        "payone-gmbh/magento-2": "^2.7",
        "raiym/instagram-php-scraper": "^0.9.2",
        "scandipwa/slider": "^1.0",
        "sharpmonks/module-payment-filter": "^1.0",
        "smile/elasticsuite": "2.8.4",
        "smile/module-product-label": "^1.1",
        "snmportal/module-syntaxhighlighter": "^2.4",
        "splendidinternet/mage2-locale-de-de": "^1.54",
        "trustedshops/trustedshops": "^2.0",
        "ulmod/module-newsletter-popup": "^2.2"
    },
    "require-dev": {
        "allure-framework/allure-phpunit": "~1.2.0",
        "friendsofphp/php-cs-fixer": "~2.13.0",
        "lusitanian/oauth": "~0.8.10",
        "magento/magento-coding-standard": "~1.0.0",
        "magento/magento2-functional-testing-framework": "~2.3.14",
        "pdepend/pdepend": "2.5.2",
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "~6.5.0",
        "sebastian/phpcpd": "~3.0.0",
        "squizlabs/php_codesniffer": "3.3.1"
    },
    "conflict": {
        "gene/bluefoot": "*"
    },
    "autoload": {
        "psr-4": {
            "Magento\\Framework\\": "lib/internal/Magento/Framework/",
            "Magento\\Setup\\": "setup/src/Magento/Setup/",
            "Magento\\": "app/code/Magento/",
            "Zend\\Mvc\\Controller\\": "setup/src/Zend/Mvc/Controller/"
        },
        "psr-0": {
            "": [
                "app/code/",
                "generated/code/"
            ]
        },
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ],
        "exclude-from-classmap": [
            "**/dev/**",
            "**/update/**",
            "**/Test/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/"
        }
    },
    "version": "2.3.2",
    "minimum-stability": "stable",
    "repositories": {
        "magento": {
            "type": "composer",
            "url": "https://repo.magento.com"
        },
        "swissup": {
            "type": "composer",
            "url": "https://docs.swissuplabs.com/packages/"
        }
    },
    "extra": {
        "magento-force": "override"
    }
}
